<html>
  <body>
    <h1>Values</h1>
    <table>
      <tr>
        <th>Key</th>
        <th>Value</th>
      </tr>
      <% values.each do |key, value| %>
        <tr>
          <td><%= key %></td>
          <td><%= value %></td>
        </tr>
      <% end %>
    </table>


    <h1>Children</h1>
    <ul>
      <% links.each do |key, value| %>
        <li>
          <a href="<%= value %>"><%= key %></a>
        </li>
      <% end %>
    </ul>
  </body>
</html>
